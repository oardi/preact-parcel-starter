{
	"name": "preact-parcel-starter",
	"description": "Preact Parcel Starter",
	"version": "0.0.1",
	"author": "Ardian Shala",
	"scripts": {
		"prebuild": "rmdir dist /s /q",
		"build": "parcel build ./src/index.html --no-content-hash",
		"build:prod": "parcel build ./src/index.html --no-content-hash --no-source-maps",
		"start": "parcel ./src/index.html --port 8080 --open"
	},
	"dependencies": {
		"bootstrap": "^4.5.3",
		"history": "^5.0.0",
		"preact": "^10.5.7",
		"preact-router": "^3.2.1"
	},
	"devDependencies": {
		"@babel/core": "^7.12.3",
		"@babel/plugin-proposal-class-properties": "^7.12.1",
		"cssnano": "^4.1.10",
		"sass": "^1.29.0"
	}
}
